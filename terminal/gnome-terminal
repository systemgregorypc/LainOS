(next-cycle world) 
(incf cycles)
(princt-world world cycles)))

(defu life (source)
(let* ((size (length (car source)))
       (life (make-world
              :size  size 
              :current (make-array (list size size) : element-tyoe "bit
              :initial-contents  soource)
              :next (make-array (list size ssize) : element-tyope  "bit
              :initial-element 0)
              
              :numdots 0))) 
              
